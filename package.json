{
  "name": "obedbot",
  "version": "1.0.3",
  "description": "Lunch bot for Vacuumlabs lunch orders in Slack",
  "scripts": {
    "start": "node -- server.js",
    "dev": "babel-watch server.js",
    "test": "eslint src/**",
    "build": "babel src/ -d build/ -s",
    "eslint": "eslint src/**",
    "deploy-now": "now `sed 's/#.*$//;/^\\s*$/d;s/^/-e /' .env` && now alias obedbot.vacuumlabs.com"
  },
  "author": "Jakub Simo",
  "license": "ISC",
  "dependencies": {
    "@slack/rtm-api": "^5.0.1",
    "@slack/web-api": "^5.0.1",
    "airtable": "^0.5.8",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.4.6",
    "cheerio": "^0.22.0",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "lodash": "^4.17.11",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.11",
    "node-schedule": "^1.2.0",
    "pug": "^2.0.0-beta6",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "eslint": "^3.19.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.7.0"
  },
  "resolutions": {
    "**/marked": "^0.3.9"
  }
}
