{
  "name": "obedbot",
  "version": "1.0.3",
  "description": "Lunch bot for Vacuumlabs lunch orders in Slack",
  "scripts": {
    "start": "node -- server.js",
    "dev": "nodemon --exec babel-node server.js",
    "test": "eslint *.js src/**/*.js && prettier --check *.js src/**/*.js",
    "build": "babel src/ -d build/ -s",
    "eslint": "eslint *.js src/**/*.js",
    "prettier": "prettier --write *.js src/**/*.js",
    "deploy-now": "now `sed 's/#.*$//;/^\\s*$/d;s/^/-e /' .env` && now alias obedbot.vacuumlabs.com"
  },
  "author": "Jakub Simo",
  "license": "ISC",
  "dependencies": {
    "@slack/rtm-api": "^5.0.1",
    "@slack/web-api": "^5.0.1",
    "airtable": "^0.5.8",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.4.6",
    "cheerio": "^0.22.0",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "lodash": "^4.17.13",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.11",
    "node-schedule": "^1.2.0",
    "nodemon": "^1.19.2",
    "pug": "^2.0.0-beta6",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.26.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "prettier": "^1.18.2"
  },
  "resolutions": {
    "**/marked": "^0.3.9"
  }
}
